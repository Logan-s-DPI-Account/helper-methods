<h1>Edit Director</h1>

<% @director.errors.full_messages.each do |message| %>
  <p style="color: red;"><%= message %></p>
<% end %>

    <%=form_with(model: @director, method: :patch) do |form|%>
<div>
<%= form.label :name %>

<%= form.text_field :name %>
</div>
<div>
  <%= form.label :date %>
  <%= form.date_field :date %>
 
  <%= form.button %>
 
  <% end%>
 </form>
   </div>
